{
  "manifest_version": 3,
  "name": "Chrome Extention ",
  "description": "React Chrome Extention",
  "version": "1",
  "action":{
    "default_popup": "popup.html",
    "default_title": "Open"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"],
    "all_frames": true,
    "match_about_blank": true
  }],
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "identity", 
    "tabs",
    "activeTab"
  ],
  "background": {"service_worker": "background.js"
      
}
}
